{
  "name": "ires-language-support",
  "displayName": "IR-ES Language Support",
  "repository": "https://github.com/ku-plrg/ires-highlighter",
  "description": "Highlighter for ESMeta IR language IR-ES",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ires",
      "aliases": ["IR-ESMeta", "ires"],
      "extensions": [".ir"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ires",
      "scopeName": "source.ir",
      "path": "./syntaxes/ires.tmLanguage.json"
    }]
  }
}
